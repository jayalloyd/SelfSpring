<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="alien" class="com.meenuscuisines.Alien" autowire="byType">

    <property name="age" value="33"  />
<property name="com"  ref="com"/>
    <constructor-arg  value="21" />
    <constructor-arg ref="laptop" />
    <constructor-arg ref="desktop" />
    <constructor-arg  ref="com2" type="com"/>

    <property name="age" value="21"/>
    <property name="lap" ref="lap1"/>
</bean>
<!--    <bean id="com1" class="com.meenuscuisines.Laptop"  primary="true" lazy-init="true" />-->
    <bean id="com1" class="com.meenuscuisines.Laptop"  />
        <bean id="com2" class="com.meenuscuisines.Desktop"  />


</beans>